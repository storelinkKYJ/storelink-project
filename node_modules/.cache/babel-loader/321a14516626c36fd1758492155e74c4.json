turn this.$route.params.filter\n    },\n    baseUrl () {\n      const path = this.$route.path\n      return path.slice(0, path.lastIndexOf('/'))\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views/apps/todo"}]}