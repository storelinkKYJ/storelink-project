iption: Tour Component\n    ----------------------------------------------------------------------------------------\n    Item Name: Vuexy - Vuejs, HTML & Laravel Admin Dashboard Template\n      Author: Pixinvent\n    Author URL: http://www.themeforest.net/user/pixinvent\n========================================================================================== -->\n\n<template>\n    <v-tour name=\"vuexyTour\" :steps=\"steps\">\n        <template slot-scope=\"tour\">\n            <transition name=\"fade\">\n                <v-step\n                    v-if=\"tour.currentStep === index\"\n                    v-for=\"(step, index) of tour.steps\"\n                    :key=\"index\"\n                    :step=\"step\"\n                    :previous-step=\"tour.previousStep\"\n                    :next-step=\"tour.nextStep\"\n                    :stop=\"tour.stop\"\n                    :is-first=\"tour.isFirst\"\n                    :is-last=\"tour.isLast\"\n                    :labels=\"tour.labels\">\n\n                    <div slot=\"actions\" class=\"flex justify-center\">\n                        <vs-button\n                            size=\"small\"\n                            class=\"mr-3\"\n                            @click=\"tour.stop\"\n                            icon-pack=\"feather\"\n                            icon=\"icon-x\"\n                            icon-after\n                            color=\"#fff\"\n                            type=\"border\"\n                            v-if=\"tour.currentStep != tour.steps.length - 1\">\n                            Skip\n                        </vs-button>\n\n                        <vs-button\n                            size=\"small\"\n                            @click=\"tour.previousStep\"\n                            icon-pack=\"feather\"\n                            icon=\"icon-chevrons-left\"\n                            color=\"#fff\"\n                            type=\"border\"\n                            class=\"mr-3\"\n                            v-if=\"tour.currentStep\">\n                            Previous\n                        </vs-button>\n\n                        <vs-button\n                            size=\"small\"\n                            @click=\"tour.nextStep\"\n                            icon-pack=\"feather\"\n                            icon=\"icon-chevrons-right\"\n                            icon-after\n                            color=\"#fff\"\n                            type=\"border\"\n                            class=\"btn-tour-next\"\n                            v-if=\"tour.currentStep != tour.steps.length - 1\">\n                            Next\n                        </vs-button>\n\n                        <vs-button\n                            size=\"small\"\n                            @click=\"tour.stop\"\n                            icon-pack=\"feather\"\n                            icon=\"icon-check-circle\"\n                            icon-after\n                            color=\"#fff\"\n                            type=\"border\"\n                            class=\"btn-tour-finish\"\n                            v-if=\"tour.currentStep == tour.steps.length - 1\">\n                            Finish\n                        </vs-button>\n                    </div>\n\n                </v-step>\n            </transition>\n        </template>\n    </v-tour>\n</template>\n\n<script>\nexport default{\n  name: 'vx-tour',\n  props: {\n    steps: {\n      required: true,\n      type: Array\n    }\n  },\n  watch: {\n    '$route.path' () {\n      this.$tours['vuexyTour'].stop()\n    }\n  },\n  mounted () {\n    this.$tours['vuexyTour'].start()\n  }\n}\n</script>\n\n<style lang=\"scss\">\n.v-tour {\n    .v-step {\n        z-index: 55000;\n        background-color: rgba(var(--vs-primary),1);\n        border-radius: .5rem;\n        padding: 1.5rem;\n        filter: drop-shadow(0 0 7px rgba(0,0,0,.5));\n\n        .v-step__arrow {\n            border-color: rgba(var(--vs-primary),1);\n        }\n\n        .vs-button-border:not(.btn-tour-next):not(.btn-tour-finish) {\n            border-color: rgba(255, 255, 255, .5) !important;\n        }\n    }\n}\n</style>\n"]}]}